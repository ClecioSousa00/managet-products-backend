{
  "$schema": "https://biomejs.dev/schemas/2.1.2/schema.json",
  "extends": [
    "ultracite"
  ],
  "linter": {
    "rules": {
      "suspicious": {
        "useAwait": "off",
        "noConsole": "off"
      },
      "complexity": {
        "noStaticOnlyClass": "off",
        "noBannedTypes": "off"
      },
      "style": {
        "noParameterProperties": "off"
      },
      "performance": {
        "noNamespaceImport": "off"
      }
    }
  }
}